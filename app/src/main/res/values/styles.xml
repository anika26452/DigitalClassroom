<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Theme</item>
        <item name="colorPrimaryDark">@color/inside</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="NoActionBar" parent="Theme.AppCompat.Light.NoActionBar"></style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"></style>
    <style name="CustomDialog" parent="@style/Theme.AppCompat.Light.Dialog">
        <item name="android:windowNoTitle">false</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="Theme.AppCompat.Light"></style>

</resources>
